    <body id="home" class="wide">
        <!-- WRAPPER -->
        <main class="wrapper"> 
            <!-- CONTENT AREA -->
            <article class="home-three">
                    <% include menu %>
                <!--Breadcrumb Section Start-->
                <section class="breadcrumb-bg bg-4 white-color">   
                    <div class="site-breadcumb ">  
                        <div class="container ">     
                            <div class="title-wrap-2">
                                <h2 class="section-title"> Inscription </h2>
                            </div>
                        </div> 
                    </div>
                    <hr class="divider-1">
                    <div class="container ">    
                        <ol class="breadcrumb breadcrumb-menubar">
                            <li> <a href="/"> Accueil </a>Inscription </li>                             
                        </ol>
                    </div>
                </section>
                <!--Breadcrumb Section End-->

                <!-- My Account Starts-->
                <section class="account-page sec-space-top pb-70">
                    <div class="container"> 
                        <div class="row">  
                            <aside class="col-md-9 col-sm-8 ptb-15">
                                <div class="account-wrap cart-box">
                                        <% if (message.length > 0) { %>
                                            <div class="alert alert-danger"><%= message %></div>
                                        <% } %>
                                    <form class="gray-control" method="POST">
                                        <div class="row">
                                            <div class="form-group col-sm-6">
                                                <label> * Nom </label>
                                                <input type="text" class="form-control" name="i_nom"  required="">                                               
                                            </div>
                                            <div class="form-group col-sm-6">
                                                <label> * Prenom </label>
                                                <input type="text" class="form-control" name="i_prenom"  required="">                                               
                                            </div>
                                            <div class="form-group col-sm-6">
                                                <label> * Email </label>
                                                <input type="email" class="form-control" name="i_email"  required="">                                               
                                            </div>                                        
                                            <div class="form-group col-sm-6">
                                                <label> Mot de passe</label>
                                                <input type="password" class="form-control" name="i_mdp" required="">                                               
                                            </div>
                                            <div class="form-group col-sm-6">
                                                <label> Telephone</label>
                                                <input type="tel" class="form-control" name="i_tel" >                                               
                                            </div>
                                            <div class="pt-15 col-sm-12">                                               
                                                <button type="submit" class="theme-btn btn-black"> <b> S'inscrire </b>                                      
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </aside>                        

                        

                        </div>
                    </div>
                </section>
                <!-- / My Account Ends -->   

                <hr class="divider-1">
                
             


            </article>
           
           

            <div id="to-top" class="to-top"> <i class="fa fa-angle-up"></i> </div>

