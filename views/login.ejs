    <body id="home" class="wide">
          
        <!-- WRAPPER -->
        <main class="wrapper"> 
            <section class="coming-wrap sec-space">
                <div class="container rel-div">
                    <div class="col-lg-6 col-lg-offset-3 text-center">
                        <div class="pb-50">         
                                <ol class="breadcrumb breadcrumb-menubar">
                                        <a href="/" style="margin-left: -1100px;"> > Accueil </a>                           
                                       </ol>                              
                            <a href="index.html" class="logo"> <img src="/assets/b_assets/images/logo/logo-1.png" alt="logo"> </a>                               
                        </div>
                        <div class="title-wrap"> 
                            <h2 class="section-title">  Bienvenue!  </h2>
                            <p> Connectez-vous à votre compte ici: </p>
                        </div>
                        <% if (loginMessage.length > 0) { %>
                            <div class="alert alert-danger"><%= loginMessage %></div>
                        <% } %>
                        <div class="login pb-50">
                            <form class="reply-form" method="POST">
                                <div class="row">
                                    <div class="form-group col-md-12">
                                        <input placeholder="Email" required="" title="" data-placement="bottom" data-toggle="tooltip" value="" name="l_email" class="form-control name input-your-name" type="text">                                               
                                    </div>
                                    <div class="form-group col-md-12">
                                        <input placeholder="Mot de passe" required="" title="" data-placement="bottom" data-toggle="tooltip" value="" name="l_pass" class="form-control name input-your-name" type="password">                                               
                                    </div>
                                    <div class="form-group col-md-12 remember-me clearfix">
                                        
                                        <a href="/forgot" class="">Mot de passe oublié?</a>
                                    </div>
                                   
                                    <div class="pt-15 col-sm-12">                                               
                                        <button class="theme-btn" type="submit"> Se connecter </button><a href="/auth/facebook" class="btn btn-primary" style="display: inline-block;
                                        height: 50px;
                                        margin: 5px;
                                        padding: 10px;
                                        width: 150px;">
                                        <span class="fa fa-facebook"></span> Facebook</a><a href="/auth/google" class="btn btn-danger" style="display: inline-block;
                                        height: 50px;
                                        margin: 5px;
                                        padding: 10px;
                                        width: 150px;"><span class="fa fa-google-plus"></span> Google</a>                                     
                                    </div>
                                </div>
                            </form>
                        </div>                       
                    </div>
