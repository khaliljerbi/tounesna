<head>
        <meta charset="utf-8">
        <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge"><![endif]-->
        <meta name="viewport" content="width=device-width, initial-scale=1">
      

        <title>Tounesna</title>

        <!-- Favicon -->
        <link rel="shortcut icon" href="/assets/b_assets/ico/favicon.ico">
        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />


        <!-- Font Icon -->
        <link href="/assets/b_assets/css/plugin/font-awesome.min.css" rel="stylesheet" type="text/css">  

        <!-- CSS Global -->
        <link rel='stylesheet' href='/assets/b_assets/css/plugin/bootstrap.min.css'>
        <link rel='stylesheet' href='/assets/b_assets/css/plugin/bootstrap-select.min.css'>
        <link rel='stylesheet' href='/assets/b_assets/css/plugin/datepicker.css'>
        <link rel='stylesheet' href='/assets/b_assets/css/plugin/owl.carousel.min.css'>        
        <link rel='stylesheet' href='/assets/b_assets/css/plugin/animate.css'>        
        <link href="/assets/b_assets/css/plugin/magnify.css" rel="stylesheet" type="text/css">  
          

        <!-- Custom CSS -->
        <link href="/assets/b_assets/css/style.css" rel="stylesheet" type="text/css">
        

        <!-- Color CSS --> 
        <link href="/assets/b_assets/css/multicolors/theme-color.css" rel="stylesheet" id="theme-config-link">

        <!--[if lt IE 9]>
        <script src="/assets/b_assets/js/plugin/html5shiv.js"></script>
        <script src="/assets/b_assets/js/plugin/respond.js"></script>
        <![endif]-->        
    </head>

    <body id="home" class="wide">
        <!-- WRAPPER -->
        <main class="wrapper"> 
            <!-- CONTENT AREA -->
            <article class="home-three">
                <!-- Top Header-1 Start -->
                   <% include menu %>
               
                <!--Breadcrumb Section Start-->
                <section class="breadcrumb-bg bg-4 white-color">   
                    <div class="site-breadcumb ">  
                        <div class="container ">     
                            <div class="title-wrap-2">
                                <h2 class="section-title"> Monuments </h2>
                            </div>
                        </div> 
                    </div>
                    <hr class="divider-1">
                    <div class="container ">    
                        <ol class="breadcrumb breadcrumb-menubar">
                            <li> <a href="/"> Accueil </a> Gallerie  </li>                             
                        </ol>
                    </div>
                </section>
                <!--Breadcrumb Section End-->

                <!-- Page Starts-->
                <section class="sec-space-top pb-50 feature-1">
                    <div class="container"> 
                            <% if (user){ %>
                                <% if(user.grade=="1") { %>

                           <a href="ajoutmon"><button type="button" rel="tooltip" class="btn btn-info" data-original-title="" title="">
                            <i class="fa fa-home"></i> Ajouter un monument
                                <div class="ripple-container"></div></button> </a><br> 
                                <% } %>
                                <% } %>
                           <br>
                        <div class="row thumbnails">
                               
                            <!-- foreach -->
                            <% var i =1 %>
                            <% var j =1 %>
                            <% monument.forEach(function(element){ %>
                            <div class="col-lg-3 col-sm-6 col-xs-12">
                                <div class="thumbnail no-border no-padding">
                                        <% j=1 %>
                                        <% images.forEach(function(img){ %>
                                            <% if(j==i) { %>
                                    <div class="media rel-div">
                                        <img src="/image/<%=img.lien %>" alt="">
                                        <div class="caption hovered tbl-wrp">
                                            <div class="caption-wrapper text-middle">
                                                <div class="caption-inner tbl-cell">
                                                    <p class="caption-buttons popup-gallery">
                                                        <a title="<%= element.nom%>" href="/image/<%=img.lien %>" class="btn caption-link"  ><i class="fa fa-link"></i></a>
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <% } %>
                                    <% j++ %>
                              <%  }) %>
                                    <div class="caption after-media">
                                        <h3 class="fsz-15 no-margin"> <a href="/monument/<%= element._id %>"> <%= element.nom %>  </a> </h3>
                                        <h6 class="fw-600 theme-color no-margin"><%= element.ville %> </h6>
                                    
                                        <% if (user){ %>
                                            <% if(user.grade=="1") { %>
                                             <a href="/modifmon/<%= element._id %>">  <button type="button" rel="tooltip" class="btn btn-success" data-original-title="" title="">
                                                        <i class="material-icons" style="    font-size: 15px;">edit</i>
                                                    <div class="ripple-container"></div></button></a> 
                                                    <button type="button" rel="tooltip" class="btn btn-danger" data-toggle="modal" data-target="#myModal" data-original-title="" title="" >
                                                        <i class="material-icons" style="    font-size: 15px;">close</i>
                                                    <div class="ripple-container"></div></button>        
                                                    <div id="myModal" class="modal fade" role="dialog">
                                                        <div class="modal-dialog">
                                                      
                                                          <!-- Modal content-->
                                                          <div class="modal-content">
                                                            <div class="modal-header">
                                                              <button type="button" class="close" data-dismiss="modal">&times;</button>
                                                              <h4 class="modal-title">Supprimer Monument</h4>
                                                            </div>
                                                            <div class="modal-body">
                                                              <p>Êtes-vous sûr de vouloir supprimer ce monument ?</p>
                                                            </div>
                                                            <div class="modal-footer">
                                                              <button type="button" class="btn btn-default" data-dismiss="modal">Annuler</button>
                                                              <a href="/supp/<%= element._id %>"><button type="button" class="btn btn-danger"  target="_top">Supprimer</button></a>
                                                            </div>
                                                          </div>
                                                      
                                                        </div>
                                                      </div>  
                                                                                      <%   } %>
                                              <%   } %>
                                     
                                            
                                    </div>
                                </div>
                            </div>
                            <% i++; %>
                            <%   }) %>
                            
                        </div>
                    </div>
                </section>
                <!-- / Page Ends -->      

                <hr class="divider-1">
                
                <!-- Patners Start -->
                
                <!-- / Patners Ends -->  


            </article>
            <!-- / CONTENT AREA -->

            <!-- FOOTER -->
            
            <!-- /FOOTER -->

            <div id="to-top" class="to-top"> <i class="fa fa-angle-up"></i> </div>

        </main>
        <!-- /WRAPPER -->


        <!-- JS Global -->
        <script src='/assets/b_assets/js/plugin/jquery-2.2.4.min.js'></script>
        <script src='/assets/b_assets/js/plugin/bootstrap.min.js'></script>
        <script src='/assets/b_assets/js/plugin/bootstrap-select.min.js'></script>
        <script src='/assets/b_assets/js/plugin/bootstrap-datepicker.js'></script>
        <script src='/assets/b_assets/js/plugin/owl.carousel.min.js'></script>
        <script src="/assets/b_assets/js/plugin/jquery.magnify.js" type="text/javascript"></script>
        
        <script src='/assets/b_assets/js/plugin/isotope.pkgd.min.js'></script>
        <script src="/assets/b_assets/js/plugin/masonry.pkgd.min.js" type="text/javascript"></script>


        <!-- Custom JS -->   
        <script src="/assets/b_assets/js/theme.js" type="text/javascript"></script>
        
        <!--[if (gte IE 9)|!(IE)]><!-->   
        <script src="/assets/b_assets/js/jquery.cookie.js"></script> 
        <!--<![endif]-->
        
    </body>

